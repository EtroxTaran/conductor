{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UIDesignerOutput",
  "type": "object",
  "required": ["agent", "status"],
  "properties": {
    "agent": { "const": "ui_designer" },
    "status": { "enum": ["complete", "needs_review", "blocked"] },
    "components_created": { "type": "array", "items": { "type": "string" } },
    "accessibility_score": { "type": "number", "minimum": 0, "maximum": 100 },
    "design_system_compliance": { "type": "boolean" }
  }
}
